
@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="hero-wrap" style="background-image: url('../images/bg_2.jpg');"data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center">
            <div class="col-md-9 ftco-animate text-center pt-5 mt-md-5">
                <h1 class="mb-3 bread">@Glow.Resources.HomeResource.Membership</h1>
                @*<p class="breadcrumbs"><span class="mr-2"><a href="index.html">Home</a></span> <span>Contact</span></p>*@
            </div>
        </div>
    </div>
</section>

<section class="ftco-sectionn bg-light">
    <div class="container">
        <div class="row justify-content-center mb-5 pb-3">
            <div class="col-md-7 heading-section ftco-animate text-center">
                <span class="subheading"><small><i class="left-bar"></i>@Glow.Resources.HomeResource.Membership<i class="right-bar"></i></small></span>
                @*<h2 class="mb-1">@Glow.Resources.HomeResource.MembershipPlans</h2>*@
            </div>
        </div>
        <div class="row">

            <div class="col-md-4 ftco-animate">
                <div class="block-7">
                    <div class="text-center">
                        <h2 class="heading" >@Glow.Resources.HomeResource.one_month</h2>
                        <span class="price"><sup>L.E</sup> <span class="number">0</span></span>
                        @*<span class="excerpt d-block">@Glow.Resources.HomeResource.Allfeaturesareincluded</span>*@

                       
                        <a href ="" onclick = "ShowPopup();"  class="btn btn-primary d-block px-2 py-4 mb-4">@Glow.Resources.HomeResource.GetStarted</a>
                        
                      
                        <h3 class="heading-2 mb-4">@Glow.Resources.HomeResource.EnjoyAllTheFeatures</h3>

                        <ul class="pricing-text">
                            <li>Invitation</li>
                            <li>Nutrition Doctor</li>
                            <li>Inbody</li>
                            <li>Freez</li>
                            <li>Group Classes</li>
                            <li>Private Trainer</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-4 ftco-animate">
                <div class="block-7">
                    <div class="text-center">
                        <h2 class="heading">@Glow.Resources.HomeResource.three_month</h2>
                        <span class="price"><sup>L.E</sup> <span class="number">0</span></span>
                        @*<span class="excerpt d-block">All features are included</span>*@
                        <a href="#" onclick = "ShowPopup();" class="btn btn-primary d-block px-3 py-4 mb-4">@Glow.Resources.HomeResource.GetStarted</a>

                        <h3 class="heading-2 mb-4">@Glow.Resources.HomeResource.EnjoyAllTheFeatures</h3>

                        <ul class="pricing-text">
                            <li>Invitation</li>
                            <li>Nutrition Doctor</li>
                            <li>Inbody</li>
                            <li>Freez</li>
                            <li>Group Classes</li>
                            <li>Private Trainer</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-4 ftco-animate">
                <div class="block-7">
                    <div class="text-center">
                        <h2 class="heading">@Glow.Resources.HomeResource.one_Year</h2>
                        <span class="price"><sup>L.E</sup> <span class="number">0</span></span>
                        @*<span class="excerpt d-block">All features are included</span>*@
                        <a href="#" onclick = "ShowPopup();" class="btn btn-primary d-block px-3 py-4 mb-4">@Glow.Resources.HomeResource.GetStarted</a>

                        <h3 class="heading-2 mb-4">@Glow.Resources.HomeResource.EnjoyAllTheFeatures</h3>

                        <ul class="pricing-text">
                            <li>Invitation</li>
                            <li>Nutrition Doctor</li>
                            <li>Inbody</li>
                            <li>Freez</li>
                            <li>Group Classes</li>
                            <li>Private Trainer</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-5">
            <div class="col-md-4">
                <div class="services d-flex ftco-animate">
                    <div class="icon-2 d-flex justify-content-center align-items-center">
                        <span class="flaticon-ruler"></span>
                    </div>
                    <div class="text px-md-2 pl-4">
                        <h3>@Glow.Resources.HomeResource.Equipment</h3>
                        <p>@Glow.Resources.HomeResource.Equipmenttxt</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="services d-flex ftco-animate">
                    <div class="icon-2 d-flex justify-content-center align-items-center">
                        <span class="flaticon-gym"></span>
                    </div>
                    <div class="text px-md-2 pl-4">
                        <h3>@Glow.Resources.HomeResource.OPen @Glow.Resources.HomeResource.Hourse</h3>
                        <p>@Glow.Resources.HomeResource.Equipmenttxt</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="services d-flex ftco-animate">
                    <div class="icon-2 d-flex justify-content-center align-items-center">
                        <span class="flaticon-tools-and-utensils"></span>
                    </div>
                    <div class="text px-md-2 pl-4">
                        <h3>@Glow.Resources.HomeResource.Healthyfood</h3>
                        <p>@Glow.Resources.HomeResource.Healthyfoodtxt</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<script type="text/javascript">
    ShowPopup = function () {
        window.open('/Home/OpenPopup?param1=param1Value', "PopupWindow", 'width=400px,height=400px,top=150,left=250');
    }
</script>  