
<!DOCTYPE html>
<html @*dir="@(System.Threading.Thread.CurrentThread.CurrentCulture.TextInfo.IsRightToLeft? "rtl" : "ltr")"*@ >
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - GLOW GYM</title>

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/Content/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="/Content/css/animate.css">

    <link rel="stylesheet" href="/Content/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/Content/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="/Content/css/magnific-popup.css">
    <link rel="stylesheet" href="/Content/css/aos.css">
    <link rel="stylesheet" href="/Content/css/ionicons.min.css">
    <link rel="stylesheet" href="/Content/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="/Content/css/jquery.timepicker.css">

    <link rel="stylesheet" href="/Content/css/flaticon.css">
    <link rel="stylesheet" href="/Content/css/icomoon.css">




    <environment include="Development">
        @if (System.Threading.Thread.CurrentThread.CurrentCulture.TextInfo.IsRightToLeft)
        {
            <link rel="stylesheet" href="/Content/css/style-ar.css">

        }
        else
        {
            <link rel="stylesheet" href="/Content/css/style.css">
        }
    </environment>
    <title>@ViewBag.Title</title>
   
</head>
<body>

    <div class="py-1 bg-black top">
        <div class="container">
            <div class="row no-gutters d-flex align-items-start align-items-center px-md-0">
                <div class="col-lg-12 d-block">
                    <div class="row d-flex">
                        <div class="col-md   pr-4 d-flex topper align-items-center">
                            <div class="icon mr-2 d-flex justify-content-center align-items-center"><span class="icon-phone2"></span></div>
                            @Html.Action("contacts")
                        </div>
                        <div class="col-md   pr-4 d-flex topper align-items-center">
                            <div class="icon mr-2 d-flex justify-content-center align-items-center"><span class="icon-paper-plane"></span></div>
                            @Html.Action("contacts_Email")
                        </div>
                        <div class="col-md   pr-4 d-flex topper align-items-center text-lg-right justify-content-end">
                            <p class="mb-0 register-link"><span>@Glow.Resources.HomeResource.OPen</span> <span>@Glow.Resources.HomeResource.Hourse</span></p>
                        </div>


                        <div class="col-md   pr-4 d-flex topper align-items-center text-lg-right justify-content-end">
                            @if (Session["MembershipNo"] != null)
                            {
                                <div class="col-md   pr-4 d-flex topper align-items-center text-lg-right justify-content-end">
                                    <a href="@Url.Action("SignOutUser", "Accounts")"> <img src="/images/logoutico.png" width="20" alt="SignOut" title="SignOut" /></a>
                                </div>

                                <a href="@Url.Action("Account", "Users")"><p style="color:white;font-size:10px">Account</p></a>

                            }
                            else
                            {
                                @Html.ActionLink("Login/Register", null, null, new { data_toggle = "modal", data_target = "#modalLRForm" })

                                //   @Html.ActionLink("Login/Register", null, null, new { data_toggle = "modal", data_target = "#modalLRForm" })
                            }


                        </div>




                        <div class="col-md   pr-4 d-flex topper align-items-center text-lg-right justify-content-end">
                            @*   <ul>
                                    <li style="list-style-type:none ; font-weight:500">

                                    </li>
                                </ul> *@
                            <environment include="Development">
                                @if (System.Threading.Thread.CurrentThread.CurrentCulture.TextInfo.IsRightToLeft)
                                {
                                    @Html.ActionLink("English", "Change", "Home", new { languageAbbrevation = "en" }, null)
                                }
                                else
                                {
                                    @Html.ActionLink("العربية", "Change", "Home", new { languageAbbrevation = "ar" }, null)
                                }
                            </environment>



                        </div>



                    </div>
                </div>
            </div>
        </div>
    </div>

    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            @*<a class="navbar-brand" href="index.html"><span><i class="left-bar"></i>Glow Gym<i class="right-bar"></i></span></a>*@
            <a class="navbar-brand" href="index.html">
                <img src="/images/logo.PNG" width="80" height="54" />
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>
            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">

                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Home, "Index", "Home", null, new { @class = "nav-link" }) </li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.About, "About", "Home", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Classes, "Classes", "clasesses", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Membership, "Membership", "Home", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Offers, "Offers", "offers", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.News, "News", "News", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Branches, "Branches", "Branches", null, new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink(Glow.Resources.HomeResource.Contact, "ContactUs", "ContactUs", null, new { @class = "nav-link" })</li>
                    @*  <li class="nav-item">
                            <span>|</span>
                            <environment include="Development">
                                @if (System.Threading.Thread.CurrentThread.CurrentCulture.TextInfo.IsRightToLeft)
                                {
                                    @Html.ActionLink("English", "Change", "Home", new { languageAbbrevation = "en" }, null)
                                }
                                else
                                {
                                    @Html.ActionLink("العربية", "Change", "Home", new { languageAbbrevation = "ar" }, null)
                                }
                            </environment>
                        </li> *@
                </ul>




            </div>
        </div>
    </nav>
    <!-- END nav -->
    @*@Html.Partial("_Login")*@




    <div class="body-content">
        @RenderBody()


    </div>

    <footer>
        <div class="ftco-footer ftco-section ">
            <div class="overlay"></div>
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <img src="/images/logoglow.PNG" width="200" height="135" />
                        <div class="glow">GLOW GYM</div>
                    </div>
                </div>

                <div class="footer-space"></div>

                <div class="row">

                    <div class="col-md-2 col-md-offset-2"></div>
                    <div class="ftco-footer-Menu col-md-3 col-md-offset-3">
                        <ul>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Home, "Index", "Home", null, new { @class = "nav-link" })</a> </li>
                            <li><a href="">join Our Team</a></li>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Contact, "ContactUs", "ContactUs", null, new { @class = "nav-link" })</a></li>
                        </ul>

                    </div>

                    <div class="ftco-footer-Menu col-md-3 col-md-offset-3">
                        <ul>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Offers, "Offers", "offers", null, new { @class = "nav-link" })</a></li>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Membership, "Membership", "Home", null, new { @class = "nav-link" })</a></li>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Classes, "Classes", "clasesses", null, new { @class = "nav-link" })</a></li>
                        </ul>

                    </div>

                    <div class="ftco-footer-Menu col-md-3 col-md-offset-3">
                        <ul>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.About, "About", "Home", null, new { @class = "nav-link" })</a></li>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.News, "News", "News", null, new { @class = "nav-link" })</a></li>
                            <li><a href="">@Html.ActionLink(Glow.Resources.HomeResource.Branches, "Branches", "Branches", null, new { @class = "nav-link" })</a></li>
                        </ul>

                    </div>

                </div>

            </div>
        </div>

        <div class="footer-CopyRight">
            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
            @Glow.Resources.HomeResource.CopyRIght2  &copy;
            <script>document.write(new Date().getFullYear());</script> @Glow.Resources.HomeResource.CopyRIght<span> @Glow.Resources.HomeResource.GLOWGYM </span>
            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->


        </div>
    </footer>





    <script src="/Scripts/js/jquery.min.js"></script>
    <script src="/Scripts/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="/Scripts/js/popper.min.js"></script>
    <script src="/Scripts/js/bootstrap.min.js"></script>
    <script src="/Scripts/js/jquery.easing.1.3.js"></script>
    <script src="/Scripts/js/jquery.waypoints.min.js"></script>
    <script src="/Scripts/js/jquery.stellar.min.js"></script>
    <script src="/Scripts/js/owl.carousel.min.js"></script>
    <script src="/Scripts/js/jquery.magnific-popup.min.js"></script>
    <script src="/Scripts/js/aos.js"></script>
    <script src="/Scripts/js/jquery.animateNumber.min.js"></script>
    <script src="/Scripts/js/bootstrap-datepicker.js"></script>
    <script src="/Scripts/js/jquery.timepicker.min.js"></script>
    <script src="/Scripts/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="/Scripts/js/google-map.js"></script>
    <script src="/Scripts/js/main.js"></script>


</body>
</html>
